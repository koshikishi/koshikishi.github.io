{
  "name": "webdevden",
  "version": "1.6.0",
  "homepage": "https://koshikishi.github.io/",
  "license": "MIT",
  "type": "module",
  "repository": "github:koshikishi/koshikishi.github.io",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "linthtml": "linthtml source/*.html",
    "eslint": "eslint source/js/",
    "stylelint": "stylelint source/styles/**/*.scss",
    "test": "npm run editorconfig && npm run linthtml && npm run eslint && npm run stylelint",
    "build": "cross-env NODE_ENV=production gulp build",
    "start": "cross-env NODE_ENV=development gulp"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@htmlacademy/editorconfig-cli": "^2.0.5",
    "@koshikishi/eslint-config": "^1.0.1",
    "@koshikishi/linthtml-config": "^1.0.0",
    "@koshikishi/stylelint-config": "^1.0.1",
    "@linthtml/linthtml": "^0.9.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.29.3",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sharp-optimize-images": "^3.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "postcss": "^8.4.27",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.0.0",
    "sass": "^1.64.1",
    "stylelint": "^16.2.0",
    "webpack": "^5.88.2",
    "webpack-stream": "^7.0.0"
  },
  "engines": {
    "node": "^18 || ^20"
  },
  "private": true,
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not and_uc > 0",
    "not < 0.2%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/images/**/*.svg",
    "source/styles/**/*.scss"
  ]
}
