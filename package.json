{
  "name": "webdevden",
  "version": "1.8.3",
  "homepage": "https://koshikishi.github.io/",
  "bugs": {
    "url": "https://github.com/koshikishi/koshikishi.github.io/issues"
  },
  "license": "MIT",
  "author": "Anton Northern",
  "type": "module",
  "repository": "github:koshikishi/koshikishi.github.io",
  "scripts": {
    "start": "gulp start",
    "build": "gulp build",
    "preview": "npm run build && browser-sync build --cors --no-notify --no-ui",
    "lint": "run-p -c lint:**",
    "lint:spaces": "ec -exclude vendor",
    "lint:markup": "html-validator src --continue --quiet",
    "lint:html": "linthtml src/**/*.html",
    "lint:styles": "stylelint src/scss/**/*.scss",
    "lint:scripts": "eslint src/js/ --ignore-pattern **/vendor",
    "optimize": "run-p optimize:**",
    "optimize:raster": "gulp raster",
    "optimize:vector": "gulp vector"
  },
  "devDependencies": {
    "@koshikishi/eslint-config": "^3.0.0",
    "@koshikishi/linthtml-config": "^1.0.2",
    "@koshikishi/stylelint-config": "^3.1.0",
    "@linthtml/linthtml": "^0.9.6",
    "browser-sync": "^3.0.2",
    "browserslist-to-esbuild": "^2.1.1",
    "editorconfig-checker": "^5.1.5",
    "eslint": "^9.3.0",
    "gulp": "^5.0.0",
    "gulp-esbuild": "^0.12.0",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sharp-responsive": "^0.4.1",
    "gulp-svgmin": "^4.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "postcss-lightningcss": "^1.0.0",
    "postcss-normalize": "^10.0.1",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "w3c-html-validator": "^1.8.1"
  },
  "engines": {
    "node": "^18 || ^20"
  },
  "private": true,
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not and_uc > 0",
    "not < 0.2%"
  ]
}
